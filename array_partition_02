# 배열 파티션
# n개의 페어를 이용한 min(a, b)의 합으로
# 만들 수 있는 가장 큰 수를 출력하라
# >>> [1,4,3,2]
# 4
#
# 짝수 번째 값 계산

from tkinter import N
from typing import List


def arrayPairSum(self, nums: List[int]) -> int:
    sum = 0
    nums.sort()

    for i, n in enumerate(nums):
        # 짝수 번째 값의 합 계산
        if i % 2 == 0:
            sum += n


    return int

